{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "@openspace/tsconfig/vue",
    "extends": "./base.json",
    "compilerOptions": {
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "allowImportingTsExtensions": true,
        "noEmit": true,
        "jsx": "preserve",
        "jsxImportSource": "vue",
        "types": [
            "vite/client"
        ]
    }
}